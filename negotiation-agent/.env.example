# Negotiation Agent Environment Variables

# Database
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=tollbit
POSTGRES_USER=tollbit
POSTGRES_PASSWORD=tollbit123

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# LLM API Keys (at least one required)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Azure OpenAI (optional)
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_DEPLOYMENT=

# Server
PORT=3003
NODE_ENV=production
LOG_LEVEL=info

# MCP Server (set to true for stdio MCP mode)
START_MCP=false

# Negotiation Limits (optional overrides)
MAX_CONCURRENT_NEGOTIATIONS=100
NEGOTIATION_TIMEOUT_SECONDS=3600
DEFAULT_MAX_ROUNDS=10
